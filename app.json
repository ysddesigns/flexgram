{
  "expo": {
    "name": "Flexgram",
    "slug": "Flexgram",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fxLogo.png",
    "scheme": "flexgram",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yusufbyusuf.Flexgram",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fxLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yusufbyusuf.Flexgram",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/fxLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/fxLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0815095e-09cc-4f47-b55b-b8eb4a541fea"
      }
    },
    "owner": "yusufbyusuf",
    "updates": {
      "url": "https://u.expo.dev/0815095e-09cc-4f47-b55b-b8eb4a541fea"
    }
  }
}
